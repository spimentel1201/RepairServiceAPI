{
  "version": 2,
  "builds": [
    {
      "src": "dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/main.js"
    }
  ],
  "build": {
    "env": {
      "PRISMA_GENERATE": "npx prisma generate",
      "PRISMA_MIGRATE": "npx prisma migrate deploy"
    }
  }
}